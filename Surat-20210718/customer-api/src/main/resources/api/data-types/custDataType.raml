#%RAML 1.0 DataType

type:
  properties:
    customerId:
      type: string
      example: "123456"

    firstName:
      type: string
      example: "John"
    
    lastName:
      type: string
      example: "James"
    
    address:
      type: string
      example: "Oxford Street, Mumbai"
    
    age:
      type: number
      example: 30
    
    country:
      type: string
      example: "India"
